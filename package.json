      },
      {
        "command": "workbench.action.showAllEditors",
        "key": "ctrl+b",
        "linux": "ctrl+b",
        "mac": "cmd+b",
        "win": "ctrl+b"
      },
      {
        "command": "workbench.action.toggleZenMode",
        "key": "shift+f11",
        "linux": "shift+f11",
        "mac": "cmd+ctrl+shift+f",
        "win": "shift+f11"
      },
      {
        "command": "redo",
        "key": "ctrl+y",
        "linux": "ctrl+y",
        "mac": "cmd+y",
        "win": "ctrl+y"
      },
      {
        "command": "editor.action.joinLines",
        "key": "ctrl+j",
