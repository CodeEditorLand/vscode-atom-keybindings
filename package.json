{
	"name": "atom-keybindings",
	"displayName": "Atom Keymap",
	"description": "Popular Atom keybindings for Visual Studio Code",
	"scripts": {
		"open-in-browser": "vscode-test-web --extensionDevelopmentPath=. ."
	},
	"contributes": {
		"configurationDefaults": {
			"editor.formatOnPaste": true,
			"editor.multiCursorModifier": "ctrlCmd",
			"workbench.list.typeNavigationMode": "trigger"
		},
		"keybindings": [
			{
				"command": "explorer.newFile",
				"key": "a",
				"linux": "a",
				"mac": "a",
				"when": "filesExplorerFocus && !inputFocus",
				"win": "a"
			},
			{
				"command": "explorer.newFolder",
				"key": "shift+a",
				"linux": "shift+a",
				"mac": "shift+a",
				"when": "filesExplorerFocus && !inputFocus",
				"win": "shift+a"
			},
			{
				"command": "explorer.openToSide",
				"key": "ctrl+1",
				"linux": "ctrl+1",
				"mac": "cmd+1",
				"when": "filesExplorerFocus && !inputFocus",
				"win": "ctrl+1"
			},
			{
				"command": "moveFileToTrash",
				"key": "backspace",
				"linux": "backspace",
				"mac": "backspace",
				"when": "filesExplorerFocus && !inputFocus",
				"win": "backspace"
			},
			{
				"command": "filesExplorer.copy",
				"key": "ctrl+c",
				"linux": "ctrl+c",
				"mac": "cmd+c",
				"when": "filesExplorerFocus && !inputFocus",
				"win": "ctrl+c"
			},
			{
				"command": "list.collapse",
				"key": "h",
				"linux": "h",
				"mac": "h",
				"when": "filesExplorerFocus && !inputFocus",
				"win": "h"
			},
			{
				"command": "list.expand",
				"key": "l",
				"linux": "l",
				"mac": "l",
				"when": "filesExplorerFocus && !inputFocus",
				"win": "l"
			},
			{
				"command": "list.focusDown",
				"key": "j",
				"linux": "j",
				"mac": "j",
				"when": "filesExplorerFocus && !inputFocus",
				"win": "j"
			},
			{
				"command": "list.focusUp",
				"key": "k",
				"linux": "k",
				"mac": "k",
				"when": "filesExplorerFocus && !inputFocus",
				"win": "k"
			},
			{
				"command": "workbench.action.toggleZenMode",
				"key": "shift+f11",
				"linux": "shift+f11",
				"mac": "cmd+shift+ctrl+f",
				"win": "shift+f11"
			},
			{
				"command": "editor.action.moveLinesDownAction",
				"key": "ctrl+down",
				"linux": "ctrl+down",
				"mac": "ctrl+cmd+down",
				"when": "editorTextFocus",
				"win": "ctrl+down"
			},
			{
				"command": "editor.action.moveLinesUpAction",
				"key": "ctrl+up",
				"linux": "ctrl+up",
				"mac": "ctrl+cmd+up",
				"when": "editorTextFocus",
				"win": "ctrl+up"
			},
			{
				"command": "editor.action.copyLinesDownAction",
				"key": "ctrl+shift+d",
				"linux": "ctrl+shift+d",
				"mac": "cmd+shift+d",
				"when": "editorTextFocus",
				"win": "ctrl+shift+d"
			},
			{
				"command": "editor.action.deleteLines",
				"key": "ctrl+shift+k",
				"linux": "ctrl+shift+k",
				"mac": "ctrl+shift+k",
				"when": "editorTextFocus",
				"win": "ctrl+shift+k"
			},
			{
				"command": "workbench.action.toggleSidebarVisibility",
				"key": "ctrl+k ctrl+b",
				"linux": "ctrl+k ctrl+b",
				"mac": "cmd+k cmd+b",
				"win": "ctrl+k ctrl+b"
			},
			{
				"command": "workbench.action.toggleSidebarVisibility",
				"key": "ctrl+\\",
				"linux": "ctrl+\\",
				"mac": "cmd+\\",
				"win": "ctrl+\\"
			},
			{
				"command": "workbench.action.splitEditor",
				"key": "ctrl+k left",
				"linux": "ctrl+k left",
				"mac": "cmd+k left",
				"win": "ctrl+k left"
			},
			{
				"command": "workbench.action.quickOpen",
				"key": "ctrl+p",
				"mac": "cmd+t",
				"win": "ctrl+t"
			},
			{
				"command": "editor.action.formatDocument",
				"key": "shift-alt-f",
				"mac": "ctrl-alt-b"
			},
			{
				"command": "workbench.action.quickOpenNavigateNext",
				"key": "ctrl+b",
				"linux": "ctrl+b",
				"mac": "cmd+b",
				"when": "inQuickOpen",
				"win": "ctrl+b"
			},
			{
				"command": "workbench.action.editor.changeLanguageMode",
				"key": "ctrl+shift+l",
				"linux": "ctrl+shift+l",
				"mac": "ctrl+shift+l",
				"win": "ctrl+shift+l"
			},
			{
				"command": "markdown.showPreviewToSide",
				"key": "ctrl+shift+m",
				"linux": "ctrl+shift+m",
				"mac": "ctrl+shift+m",
				"win": "ctrl+shift+m"
			},
			{
				"command": "workbench.action.reloadWindow",
				"key": "alt+ctrl+r",
				"linux": "alt+ctrl+r",
				"mac": "ctrl+alt+cmd+l",
				"win": "alt+ctrl+r"
			},
			{
				"command": "editor.action.openLink",
				"key": "none",
				"mac": "ctrl+shift+o"
			},
			{
				"command": "workbench.action.toggleDevTools",
				"key": "ctrl+alt+i",
				"linux": "ctrl+alt+i",
				"mac": "alt+cmd+i",
				"win": "ctrl+alt+i"
			},
			{
				"command": "editor.action.showSnippets",
				"key": "alt+shift+s",
				"linux": "alt+shift+s",
				"mac": "alt+shift+s",
				"win": "alt+shift+s"
			},
			{
				"command": "workbench.action.files.openFolder",
				"key": "ctrl+shift+o",
				"linux": "ctrl+shift+o",
				"win": "ctrl+shift+o"
			},
			{
				"command": "workbench.action.files.openFileFolder",
				"key": "none",
				"mac": "cmd+shift+o"
			},
			{
				"command": "editor.action.jumpToBracket",
				"key": "ctrl+m",
				"linux": "ctrl+m",
				"mac": "ctrl+m",
				"when": "editorTextFocus",
				"win": "ctrl+m"
			},
			{
				"command": "expandLineSelection",
				"key": "ctrl+l",
				"linux": "ctrl+l",
				"mac": "cmd+l",
				"when": "editorTextFocus",
				"win": "ctrl+l"
			},
			{
				"command": "cursorColumnSelectUp",
				"key": "shift+alt+up",
				"linux": "shift+alt+up",
				"mac": "ctrl+shift+up",
				"when": "editorTextFocus"
			},
			{
				"command": "cursorColumnSelectDown",
				"key": "shift+alt+down",
				"linux": "shift+alt+down",
				"mac": "ctrl+shift+down",
				"when": "editorTextFocus"
			},
			{
				"command": "editor.action.format",
				"key": "ctrl+shift+i",
				"linux": "ctrl+shift+i",
				"mac": "ctrl+alt+b",
				"when": "editorHasFormattingProvider && editorTextFocus && !editorReadonly",
				"win": "alt+shift+f"
			},
			{
				"command": "workbench.action.terminal.toggleTerminal",
				"key": "ctrl+`",
				"linux": "ctrl+`",
				"mac": "ctrl+alt+t",
				"win": "ctrl+`"
			},
			{
				"command": "workbench.action.toggleFullScreen",
				"key": "f11",
				"linux": "f11",
				"mac": "ctrl+cmd+f",
				"win": "f11"
			},
			{
				"command": "workbench.action.gotoSymbol",
				"key": "ctrl+r",
				"linux": "ctrl+r",
				"mac": "cmd+r",
				"win": "ctrl+r"
			},
			{
				"command": "editor.fold",
				"key": "alt+ctrl+[",
				"linux": "alt+ctrl+[",
				"mac": "alt+cmd+[",
				"when": "editorTextFocus",
				"win": "alt+ctrl+["
			},
			{
				"command": "editor.unfold",
				"key": "alt+ctrl+]",
				"linux": "alt+ctrl+]",
				"mac": "alt+cmd+]",
				"when": "editorTextFocus",
				"win": "alt+ctrl+]"
			},
			{
				"command": "editor.foldAll",
				"key": "alt+ctrl+shift+[",
				"linux": "alt+ctrl+shift+[",
				"mac": "alt+cmd+shift+[",
				"when": "editorTextFocus",
				"win": "alt+ctrl+shift+["
			},
			{
				"command": "editor.unfoldAll",
				"key": "alt+ctrl+shift+]",
				"linux": "alt+ctrl+shift+]",
				"mac": "alt+cmd+shift+]",
				"when": "editorTextFocus",
				"win": "alt+ctrl+shift+]"
			},
			{
				"command": "editor.action.commentLine",
				"key": "cmd+k cmd+c",
				"mac": "cmd+shift+7",
				"when": "editorTextFocus"
			},
			{
				"command": "workbench.action.zoomIn",
				"key": "ctrl+=",
				"mac": "cmd+="
			},
			{
				"command": "workbench.action.zoomOut",
				"key": "ctrl+-",
				"mac": "cmd+-"
			},
			{
				"command": "editor.foldLevel1",
				"key": "ctrl+k ctrl+1",
				"linux": "ctrl+k ctrl+1",
				"mac": "cmd+k cmd+1",
				"when": "editorTextFocus",
				"win": "ctrl+k ctrl+1"
			},
			{
				"command": "editor.foldLevel2",
				"key": "ctrl+k ctrl+2",
				"linux": "ctrl+k ctrl+2",
				"mac": "cmd+k cmd+2",
				"when": "editorTextFocus",
				"win": "ctrl+k ctrl+2"
			},
			{
				"command": "editor.foldLevel3",
				"key": "ctrl+k ctrl+3",
				"linux": "ctrl+k ctrl+3",
				"mac": "cmd+k cmd+3",
				"when": "editorTextFocus",
				"win": "ctrl+k ctrl+3"
			},
			{
				"command": "editor.foldLevel4",
				"key": "ctrl+k ctrl+4",
				"linux": "ctrl+k ctrl+4",
				"mac": "cmd+k cmd+4",
				"when": "editorTextFocus",
				"win": "ctrl+k ctrl+4"
			},
			{
				"command": "editor.foldLevel5",
				"key": "ctrl+k ctrl+5",
				"linux": "ctrl+k ctrl+5",
				"mac": "cmd+k cmd+5",
				"when": "editorTextFocus",
				"win": "ctrl+k ctrl+5"
			},
			{
				"command": "workbench.action.nextEditor",
				"key": "ctrl+pagedown",
				"linux": "ctrl+pagedown",
				"win": "ctrl+pagedown"
			},
			{
				"command": "workbench.action.previousEditor",
				"key": "ctrl+pageup",
				"linux": "ctrl+pageup",
				"win": "ctrl+pageup"
			},
			{
				"command": "editor.action.selectHighlights",
				"key": "alt+f3",
				"linux": "alt+f3",
				"mac": "ctrl+cmd+g",
				"when": "editorFocus",
				"win": "alt+f3"
			},
			{
				"command": "editor.action.insertCursorAtEndOfEachLineSelected",
				"key": "alt+shift+l",
				"linux": "alt+shift+l",
				"mac": "cmd+shift+l",
				"when": "editorTextFocus && editorHasSelection",
				"win": "alt+shift+l"
			},
			{
				"command": "workbench.action.zoomOut",
				"key": "ctrl+-",
				"linux": "ctrl+-",
				"mac": "cmd+-",
				"win": "ctrl+-"
			},
			{
				"command": "workbench.action.openEditorAtIndex1",
				"key": "alt+1",
				"linux": "alt+1",
				"mac": "cmd+1",
				"win": "alt+1"
			},
			{
				"command": "workbench.action.openEditorAtIndex2",
				"key": "alt+2",
				"linux": "alt+2",
				"mac": "cmd+2",
				"win": "alt+2"
			},
			{
				"command": "workbench.action.openEditorAtIndex3",
				"key": "alt+3",
				"linux": "alt+3",
				"mac": "cmd+3",
				"win": "alt+3"
			},
			{
				"command": "workbench.action.openEditorAtIndex4",
				"key": "alt+4",
				"linux": "alt+4",
				"mac": "cmd+4",
				"win": "alt+4"
			},
			{
				"command": "workbench.action.openEditorAtIndex5",
				"key": "alt+5",
				"linux": "alt+5",
				"mac": "cmd+5",
				"win": "alt+5"
			},
			{
				"command": "workbench.action.openEditorAtIndex6",
				"key": "alt+6",
				"linux": "alt+6",
				"mac": "cmd+6",
				"win": "alt+6"
			},
			{
				"command": "workbench.action.openEditorAtIndex7",
				"key": "alt+7",
				"linux": "alt+7",
				"mac": "cmd+7",
				"win": "alt+7"
			},
			{
				"command": "workbench.action.openEditorAtIndex8",
				"key": "alt+8",
				"linux": "alt+8",
				"mac": "cmd+8",
				"win": "alt+8"
			},
			{
				"command": "workbench.action.openEditorAtIndex9",
				"key": "alt+9",
				"linux": "alt+9",
				"mac": "cmd+9",
				"win": "alt+9"
			},
			{
				"command": "workbench.files.action.showActiveFileInExplorer",
				"key": "ctrl+shift+\\",
				"linux": "ctrl+shift+\\",
				"mac": "alt+cmd+\\",
				"win": "ctrl+shift+\\"
			},
			{
				"command": "workbench.action.files.copyPathOfActiveFile",
				"key": "ctrl+shift+c",
				"linux": "ctrl+shift+c",
				"mac": "ctrl+shift+c",
				"when": "!terminalFocus",
				"win": "ctrl+shift+c"
			},
			{
				"command": "workbench.action.openGlobalSettings",
				"key": "ctrl+,",
				"linux": "ctrl+,",
				"mac": "cmd+,",
				"win": "ctrl+,"
			},
			{
				"command": "workbench.action.showAllEditors",
				"key": "ctrl+b",
				"linux": "ctrl+b",
				"mac": "cmd+b",
				"win": "ctrl+b"
			},
			{
				"command": "workbench.action.toggleZenMode",
				"key": "shift+f11",
				"linux": "shift+f11",
				"mac": "cmd+ctrl+shift+f",
				"win": "shift+f11"
			},
			{
				"command": "redo",
				"key": "ctrl+y",
				"linux": "ctrl+y",
				"mac": "cmd+y",
				"win": "ctrl+y"
			},
			{
				"command": "editor.action.joinLines",
				"key": "ctrl+j",
				"linux": "ctrl+j",
				"mac": "cmd+j",
				"win": "ctrl+j"
			},
			{
				"command": "workbench.action.quickOpenPreviousEditor",
				"key": "ctrl+p",
				"linux": "ctrl+p",
				"mac": "cmd+p",
				"win": "ctrl+p"
			},
			{
				"command": "list.toggleKeyboardNavigation",
				"key": "/",
				"when": "listFocus && !inputFocus && listSupportsKeyboardNavigation"
			},
			{
				"command": "workbench.action.terminal.clear",
				"key": "ctrl+k",
				"linux": "ctrl+k",
				"mac": "cmd+k",
				"when": "terminalFocus",
				"win": "ctrl+k"
			}
		]
	},
	"devDependencies": {},
	"icon": "atom-keyboard-padded.png",
	"preview": true,
	"extensionKind": [
		"ui",
		"workspace"
	]
}
